	hdd_pwr {
		compatiable = "Reatek,hdd_pwr";
		detect_hdd1_gpio = <&rtk_iso_gpio 13 0 0>;
		detect_hdd2_gpio = <&rtk_iso_gpio 7 0 0>;
		detect_hdd3_gpio = <&rtk_misc_gpio 50 0 0>;
		detect_hdd4_gpio = <&rtk_misc_gpio 8 0 0>;

		power_supply_hdd1_gpio = <&rtk_misc_gpio 49 0 0>;
