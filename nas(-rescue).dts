		power_supply_hdd3_gpio = <&rtk_misc_gpio 57 0 0>;
		power_supply_hdd4_gpio = <&rtk_misc_gpio 56 0 0>;

		hdd_asm_rst_1 = <&rtk_misc_gpio 58 0 0>;
		hdd_asm_rst_2 = <&rtk_misc_gpio 53 0 0>;
		hdd_asm_rst_3 = <&rtk_misc_gpio 54 0 0>;
		hdd_asm_rst_4 = <&rtk_misc_gpio 55 0 0>;
		
		//ERR_led = <&rtk_iso_gpio 3 0 0>;
